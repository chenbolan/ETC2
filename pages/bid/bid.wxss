.bid_head_con {
  height: 386rpx;
  overflow: hidden;
  width: 100%;
  position: relative;
}
.bid_head_con .bid_head_con_bg {
  position: absolute;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  height: 100%;
  width: 180%;
  background: #6574c7;
  border-radius: 0 0 50% 50%;
}
.bid_card {
  position: relative;
  z-index: 1000;
  margin: 44rpx auto 0;
  width: 662rpx;
  height: 294rpx;
  background: url(data:image/jpeg;base64,) no-repeat left top;
  background-size: contain;
  border-radius: 20rpx;
}
.bid_num_con {
  padding: 27rpx 27rpx 0;
  align-items: center;
}
.bid_num_con text {
  display: block;
  font-size: 22rpx;
  color: #2a2a2a;
  line-height: 22rpx;
}

.bid_btn_con {
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  margin-top: 41rpx;
  color: #fff;
  letter-spacing: 1px;
  font-size: 42rpx;
  font-weight: bold;
  vertical-align: middle;
}
.bid_btn_con .icon1 {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  background-position: -662rpx top;
  margin-right: 18rpx;
}
.bid_btn_con .icon2 {
  width: 23rpx;
  height: 41rpx;
  display: inline-block;
  vertical-align: middle;
  background-position: -728rpx top;
  margin-left: 51rpx;
}
.more {
  text-align: right;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #676767;
  padding-right: 32rpx;
  margin-top: 32rpx;
}
.more .icon3 {
  width: 43rpx;
  height: 41rpx;
  display: inline-block;
  background-position: -86rpx -70rpx;
  margin-left: 8rpx;
}
.bid_introduce {
  width: 713rpx;
  height: 585rpx;
  margin: 43rpx auto 0;
  box-shadow: 0px 0px 20rpx #e7eafb;
  border-bottom: 10rpx solid #7061e3;
  border-radius: 10rpx;
}
.bid_introduce_h {
  text-align: center;
  font-size: 36rpx;
  line-height: 36rpx;
  color: #696969;
  padding-top: 62rpx;
  font-weight: bold;
}
.condition {
    text-align: center;
    font-size: 26rpx;
    line-height: 26rpx;
    color: #6a6a6a;
    margin-top: 37rpx;
}
.advantage {
  text-align: center;
  margin-top: 38rpx;
  vertical-align: middle;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #585a5a;
}
.advantage text {
  display: inline-block;
  padding: 23rpx 18rpx;
  background: #fee8eb;
  border-radius: 74rpx;
}
.advantage .middle_advantage {
  margin: 0 55rpx;
}
.introduce_t {
  margin-top: 40rpx;
  margin-left: 32rpx;
  border-left: 4rpx solid #6c5aee;
  padding-left: 20rpx;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 22rpx;
  color: #959595;
}
.certificates {
  text-align: center;
  margin-top: 40rpx;
}
.certificates view {
  display: inline-block;
}
.certificates_v_con view{
  width: 196rpx;
  height: 121rpx;
  margin-bottom: 28rpx;
  background-position: -243rpx -129rpx;
}
.certificates_v_middle {
  margin: 0 35rpx;
}
.certificates_v_con2 view{
  background-position: -456rpx -129rpx;
}
.certificates_v_con3 view{
  background-position: left -276rpx;
}
.certificates text {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #6d6d6d;
  display: block;
  text-align: center;
}
.common_btn2 {
  margin-top: 72rpx;
  background: #6378ef;
  color: #fff;
  border-color: #485cd3;
  margin-bottom: 20rpx;
}